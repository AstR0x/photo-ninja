{
  "name": "photo-ninja",
  "version": "1.0.0",
  "description": "photo effects website",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix ./client",
    "client:install": "npm install --prefix ./client",
    "client:build": "npm run build --prefix ./client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstR0x/photo-ninja.git"
  },
  "keywords": [
    "photo-ninja"
  ],
  "author": "Astr0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AstR0x/photo-ninja/issues"
  },
  "homepage": "https://github.com/AstR0x/photo-ninja#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "config": "^3.2.5",
    "cross-env": "^6.0.3",
    "nodemon": "^2.0.2"
  }
}
